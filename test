<!DOCTYPE html>
<html>
<head>
  <title>Response Time Test</title>
  <style>
    #image-container {
      display: flex;
      justify-content: space-around;
    }
    img {
      cursor: pointer;
      width: 200px;
      height: 200px;
    }
  </style>
</head>
<body>
  <h1>Response Time Test</h1>
  <div id="image-container">
    <img id="left-image" src="image1.jpg" alt="Image 1">
    <img id="right-image" src="image2.jpg" alt="Image 2">
  </div>
  <p id="result"></p>

  <script>
    let startTime;
    const leftImage = document.getElementById('left-image');
    const rightImage = document.getElementById('right-image');
    const result = document.getElementById('result');

    function startTest() {
      startTime = new Date().getTime();
    }

    function endTest() {
      const endTime = new Date().getTime();
      const responseTime = endTime - startTime;
      result.textContent = `Response time: ${responseTime} ms`;
    }

    leftImage.addEventListener('click', endTest);
    rightImage.addEventListener('click', endTest);

    window.onload = startTest;
  </script>
</body>
</html>
